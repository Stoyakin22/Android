fun main() {
    var averageAge = 30 // Возраст Иннокентия

    val numberOfDevelopers = 4 // Количество разработчиков (Иннокентий + 3)

    val totalAge = averageAge + 25 + 36 + 21

    averageAge = totalAge / numberOfDevelopers

    println("Средний возраст разработчика в команде Иннокентия: $averageAge")
}
